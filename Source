local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Emergency Hamburg", "Synapse")



---MAIN

local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Buy Stuff")


MainSection:NewButton("Buy Glock 17", "Buys A Glock 17 From Dealer", function()
  
local args = {
    [1] = "Glock 17",
    [2] = "Dealer"
}

game:GetService("ReplicatedStorage")["events-rx1"].buyItem:FireServer(unpack(args))



end)

MainSection:NewButton("Buy M4 Carbine", "Buys M4 From Dealer", function()
    
local args = {
    [1] = "M4 Carbine",
    [2] = "Dealer"
