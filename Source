local MainSection = Main:NewSection("Sell")


MainSection:NewButton("Sell Stolen Items", "Sells Gold Diamond Jewelry", function()
    local args = {
        [1] = "Jewelry",
        [2] = "Dealer"
    }
    
    game:GetService("ReplicatedStorage")["events-rx1"].sellItem:FireServer(unpack(args))
    
    
    
    
    
    
    
    local args = {
        [1] = "Diamond",
        [2] = "Smuggler"
    }
    
    game:GetService("ReplicatedStorage")["events-rx1"].sellItem:FireServer(unpack(args))
    
    
    local args = {
        [1] = "Gold",
        [2] = "Dealer"
    }
    
    game:GetService("ReplicatedStorage")["events-rx1"].sellItem:FireServer(unpack(args))
    
    
    
    
    
end)
